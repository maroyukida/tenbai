{
  "expo": {
    "name": "Pocchari Chat",
    "slug": "pocchari-chat",
    "version": "1.0.0",
    "orientation": "portrait",
    "jsEngine": "hermes",
    "newArchEnabled": true,
    "scheme": "himachat",
    "platforms": ["ios", "android", "web"],
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#fff8fb"
    },
    "android": {
      "package": "com.example.pocchari",
      "versionCode": 1
    },
    "ios": {
      "bundleIdentifier": "com.example.pocchari",
      "buildNumber": "1"
    },
    "extra": {
      "firebase": {
        "apiKey": "YOUR_API_KEY",
        "authDomain": "YOUR_AUTH_DOMAIN",
        "projectId": "YOUR_PROJECT_ID",
        "storageBucket": "YOUR_STORAGE_BUCKET",
        "messagingSenderId": "YOUR_MESSAGING_SENDER_ID",
        "appId": "YOUR_APP_ID"
      },
      "useEmulator": false
    }
  }
}
